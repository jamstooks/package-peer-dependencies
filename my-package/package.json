{
  "name": "my-package",
  "version": "0.0.1",
  "description": "peer/dev dependency testing",
  "main": "dist/index.js",
  "repository": "https://github.com/jamstooks/package-peer-dependencies",
  "license": "none",
  "private": true,
  "scripts": {
    "build": "yarn build:babel",
    "build:fresh": "yarn clean:dist && yarn build && yarn clean:modules",
    "build:babel": "babel ./src/ --out-dir ./dist --copy-files",
    "clean:dist": "yarn rimraf ./dist",
    "clean:modules": "yarn rimraf ./node_modules",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "jest": "^24.8.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "rimraf": "^3.0.0"
  },
  "peerDependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ]
  }
}
